{
  "name": "chat-changing",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "homepage": "https://1noy.github.io/Cirus/",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "predeploy": "npm run build:prod",
    "deploy": "gh-pages -d dist",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,md}\"",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "type-check": "tsc --noEmit",
    "bundle-analyzer": "npm run build:analyze && npx vite-bundle-analyzer dist",
    "performance": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "audit": "npm audit && npm audit fix"
  },
  "dependencies": {
    "@emoji-mart/react": "^1.1.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5.18.0",
    "@mui/material": "^5.18.0",
    "canvas": "^3.1.2",
    "emoji-picker-react": "^4.13.2",
    "firebase": "^12.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "cssnano": "^6.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.1.1",
    "jest": "^29.7.0",
    "lighthouse": "^11.6.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "terser": "^5.27.2",
    "vite": "^5.1.4",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-compression": "^0.5.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "resolutions": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
